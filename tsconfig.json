{
  "compilerOptions": {
    "moduleResolution": "node",
    "esModuleInterop": true,
    "outDir": "./dist/",
    "noImplicitAny": true,
    "module": "ES2015",
    "target": "ES5",
    "jsx": "react",
    "lib": [
      "esnext",
      "dom"
    ],
    "allowJs": true,
    "baseUrl": "./",
    "paths": {
      "@components/*": ["src/components/*"],
      "@configs/*": ["src/configs/"],
      "@hooks/*": ["src/hooks/*"],
      "@routes/*": ["src/routes/"],
      "@pages/*": ["src/pages/*"],
      "@common/*": ["src/common/*"],
      "@utils/*": ["src/utils/*"],
    },
  },
  "exclude": [
    "node_modules",
    "dist",
  ],
  
}